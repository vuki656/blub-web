{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "lib": ["dom", "dom.iterable", "esnext", "es5"],
        "skipLibCheck": true,
        "target": "es5",
        "types": ["cypress", "node"],
        "noEmit": true,
        "incremental": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve"
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
}
