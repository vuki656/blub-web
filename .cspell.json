{
    "useGitignore": true,
    "cache": {
        "useCache": true,
        "cacheStrategy": "content",
        "cacheLocation": "./.cspellcache"
    },
    "ignorePaths": ["./public/**/*"],
    "ignoreWords": [
        "blub",
        "codegen",
        "mantine",
        "tabler",
        "hookform",
        "rimac",
        "pageview",
        "autosize",
        "loglevel",
        "hookz",
        "yarnrc",
        "eslintcache",
        "anonymous",
        "nextjs",
        "whatsonyourmind",
        "blubtalk",
        "ccpa",
        "screenshotted",
        "tiktok"
    ]
}
