        "tsyringe",
        "pino",
        "envalid",
        "ossp",
        "typegraphql",
        "nodemon",
        "yarnrc",
        "varchar",
        "datetime",
        "liquibase",
        "jdbc",
        "healthcheck",
        "datasource",
